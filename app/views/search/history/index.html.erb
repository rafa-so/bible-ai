<div class="container">
    <% if @history.empty? %>
        <p> Não existe histórico ainda </p>
    <% else  %>
        <table class="search-response">
            <% @history.each do |item| %>
                <tr>
                    <td>
                        <%= link_to item.term, history_url(item.id) %>
                    </td>
                </tr>
            <% end %>
        <table>
    <% end %>
</div>
